{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@newrelic/browser-agent": "^1.297.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env PORT=3500 react-scripts start --host 0.0.0.0",
    "build": "react-scripts build --verbose --stats=verbose",
    "test": "react-scripts test",
    "test:list": "set CI=true&& set TEMP=.tmp&& set TMP=.tmp&& set JEST_CACHE_DIRECTORY=.jest-cache&& react-scripts test --no-cache --listTests --watchAll=false",
    "test:ci": "react-scripts test --ci --reporters=default --reporters=jest-junit",
    "test:e2e": "playwright test --reporter=junit --output=playwright-report --project=chromium",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "cross-env": "^7.0.3",
    "http-proxy-middleware": "^3.0.5",
    "jest-junit": "^16.0.0",
    "typescript": "^4.9.5"
  },
  "jest-junit": {
    "output": "./junit.xml"
  }
}
